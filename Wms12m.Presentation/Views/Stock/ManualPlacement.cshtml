@{
    ViewBag.Title = "Stok Ekle";
    Layout = "~/Views/Shared/_Layout.cshtml";
}<div class="page-bar"><ul class="page-breadcrumb"><li><a href="/">Anasayfa</a><i class="fa fa-circle"></i></li><li><a href="/Stock">Stok</a><i class="fa fa-circle"></i></li><li><span>Ekle</span></li></ul></div>
<h1 class="page-title">Stok Ekle</h1>
<div class="row">
    <div class="col-md-12">
        <div>
            <div class="portlet box green">
                <div class="portlet-title">
                    <div class="caption">
                        <i class="fa fa-gift"></i>Stok Ekle
                    </div>
                </div>
                <div class="portlet-body form">
                    <div class="form-horizontal">
                        <div class="form-body">
                            <div class="form-group">
                                <div class="col-sm-3 control-label">Depo</div>
                                <div class="col-sm-4">
                                    @Html.DropDownList("DepoID", null, htmlAttributes: new { @class = "form-control StoreListi input-circle", @onchange = "getCorridor(this.value);" })
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-sm-3 control-label">Koridor</div>
                                <div class="col-sm-4">
                                    <select id="KoridorID" class="form-control input-circle" onchange="getShelf(this.value);"></select>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-sm-3 control-label">Raf</div>
                                <div class="col-sm-4">
                                    <select id="RafID" class="form-control input-circle" onchange="FuncSearchBtn();"></select>
                                </div>
                            </div>
                            <div id="Grid" class="p-30"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@section Scripts{

}