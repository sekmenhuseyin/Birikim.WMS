@model List<Wms12m.Entity.Store05>
<script type="text/javascript">
    $(".make-switch").bootstrapSwitch();
</script>
@if (((List<Wms12m.Entity.Store05>)Model).Count() > 0)
{
    <table class="table table-striped table-bordered table-hover" id="sample_1">
        <thead>
            <tr>
                <th> Kat</th>
                <th> Sıra No</th>
                <th> Tarih </th>
                <th> Durum </th>
                <th> Düzenle </th>
                <th> Sil </th>
            </tr>
        </thead>
        <tbody>
            @foreach (Wms12m.Entity.Store05 item in Model)
            {
                <tr>
                    <td>@item.FloorName</td>
                    <td>@item.Sort </td>
                    <td>@((item.CreateDate).Value.ToString("dd-MM-yyyy HH:mm:ss"))</td>
                    <td>@(item.Locked ? "Aktif" : "Pasif")</td>
                    <td><a class="edit" href="javascript:;" onclick="CreateEdit('@item.Id')"> Düzenle </a></td>
                    <td>
                        <a class="edit" href="javascript:;" onclick="Delete('@item.Id','/Floor/FloorGridPartial','FloorGrid','@item.ChapterId')"> Sil </a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <div>
        Kayıt Bulunamadı
    </div>
}

<script src="~/Content/assets/global/scripts/datatable.js" type="text/javascript"></script>
<script src="~/Content/assets/global/plugins/datatables/datatables.min.js" type="text/javascript"></script>
<script src="~/Content/assets/global/plugins/datatables/plugins/bootstrap/datatables.bootstrap.js" type="text/javascript"></script>
<script src="~/Content/assets/global/plugins/bootstrap-datepicker/js/bootstrap-datepicker.min.js" type="text/javascript"></script>
