@model IEnumerable<Message>
@{
    ViewBag.Title = "Bildirimler";
    Layout = "~/Views/Shared/_Layout.cshtml";
}<div class="page-bar"><ul class="page-breadcrumb"><li><a href="/">Anasayfa</a><i class="fa fa-circle"></i></li><li><span>Bildirimler</span></li></ul></div>
<h1 class="page-title">Bildirimler</h1>
<div class="row">
    <div class="col-md-12">
        <div class="portlet box green">
            <div class="portlet-title">
                <div class="caption">
                    <i class="fa fa-gift"></i>Bildirimler
                </div>
            </div>
            <div class="portlet-body form">
                <div id="liste"></div>
            </div>
        </div>
    </div>
</div>

<h2>Notifications</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Tarih)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Kimden)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Kime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Mesaj)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Tarih)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Kimden)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Kime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Mesaj)
        </td>
        <td>
        </td>
    </tr>
}

</table>
