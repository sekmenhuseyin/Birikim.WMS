@model frmMalzeme
<tr> 
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    <td>@Html.TextBoxFor(a => a.MalKodu, new { @class = "form-control input-circle", @required = "" })</td>
    <td>@Html.TextBoxFor(a => a.MalAdi, new { @class = "form-control input-circle", @required = "" })</td>
    <td>@Html.TextBoxFor(a => a.Miktar, new { @class = "form-control input-circle", @required = "" })</td>
    <td><select name="Birim" id="Birim" class = "form-control input-circle" required></select></td>
    <td><input type="hidden" name="IrsaliyeId" id="IrsaliyeId" value="@ViewBag.IrsaliyeId" /><input type="submit" class="btn btn-primary btn-block input-circle" value="Ekle" /></td>
</tr>
<script src="~/Content/assets/global/plugins/jquery-file-upload/js/main.js"></script>
