@model IEnumerable<WMS_IRS>
<div class="modal-content">
    <div class="close-modal" data-dismiss="modal"><div class="lr"><div class="rl"></div></div></div>
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="modal-body">@*Project Details Go Here*@
                    <h4>İrsaliye Liste</h4>
                    <table class="table">
                        <tr>
                            <th>@Html.DisplayNameFor(model => model.SirketKod)</th>
                            <th>@Html.DisplayNameFor(model => model.EvrakNo)</th>
                            <th>@Html.DisplayNameFor(model => model.HesapKodu)</th>
                            <th>@Html.DisplayNameFor(model => model.Unvan)</th>
                            <th></th>
                        </tr>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>@Html.DisplayFor(modelItem => item.SirketKod)</td>
                                <td>@Html.DisplayFor(modelItem => item.EvrakNo)</td>
                                <td>@Html.DisplayFor(modelItem => item.HesapKodu)</td>
                                <td>@Html.DisplayFor(modelItem => item.Unvan)</td>
                                <td>
                                    <input type="submit" value="Seç" class="btn btn-primary" />
                                </td>
                            </tr>
                        }
                    </table>
                </div>
                <div class="col-md-5">
                    <button type="button" class="btn btn-danger" data-dismiss="modal"><i class="fa fa-times"></i> Kapat</button>
                </div>
            </div>
        </div>
    </div>
</div>
