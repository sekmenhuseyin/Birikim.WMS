@model List<frmHesapUnvan>
<table class="table table-striped table-bordered table-hover" id="sample_1">
    <thead>
        <tr>
            <th>Hesap Kodu</th>
            <th>Ünvan</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr style="cursor:pointer;" ondblclick="hesapSelect('@item.HesapKodu','@item.Unvan ');">
                <td>@item.HesapKodu</td>
                <td>@item.Unvan</td>
            </tr>
        }
    </tbody>
</table>
<script>
    $(document).ready(function () { $('#sample_1').DataTable({ "language": { "url": "http://cdn.datatables.net/plug-ins/1.10.13/i18n/Turkish.json" } }); });
    function hesapSelect(no, ad) { $('#HesapKodu').val(no); $('#Unvan').val(ad); $('#hesapSelect').hide(); checkEnabled(); }
</script>