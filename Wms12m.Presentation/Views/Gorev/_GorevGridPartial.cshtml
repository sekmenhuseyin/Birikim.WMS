@model List<Gorev>
<table class="table table-striped table-bordered table-hover" id="sample_1">
    <thead>
        <tr>
            <th>Depo</th>
            <th>EvrakNo</th>
            <th>Görev No</th>
            <th>Görev Tipi</th>
            <th>Durum</th>
            <th>Görevli</th>
            <th>Oluşturma Tarihi</th>
            <th>Atama Tarihi</th>
            <th>Bitirme Tarihi</th>
            <th>Bilgi</th>
            <th>Açıklama</th>
            <th></th>
        </tr>
    </thead>
    <tbody id="GorevFrom">
        @foreach (var item in Model)
        {
            <tr ondblclick="tiklama('@item.IrsaliyeID')">
                <td>@item.Depo.Depo</td>
                <td>@item.IRS.EvrakNo</td>
                <td>@item.GorevNo</td>
                <td>@item.ComboItemName.Name</td>
                <td>@item.ComboItemName1.Name</td>
                <td><a class="btn btn-default input-circle" href="#modalEditPage" data-toggle="modal" onclick="editInModal('/Gorev/GorevliAta/@item.ID')"> @{if (item.USR011 != null){ @item.USR011.AdSoyad} else {<text>Görevli Ata</text>}} </a></td>
                <td>@item.OlusturmaTarihi.FromOADateInt()</td>
                <td>@{if (item.AtamaTarihi != null) { @item.AtamaTarihi.Value.FromOADateInt() } }</td>
                <td>@{if (item.BitisTarihi != null) { @item.BitisTarihi.Value.FromOADateInt() } }</td>
                <td>@item.Bilgi </td>
                <td>@item.Aciklama </td>
                <td>
                    <a class="btn btn-default" href="javascript:;" onclick="CreateEdit('@item.ID')"> Düzenle </a>
                    <a class="btn btn-danger" href="javascript:;" onclick="Delete('@item.ID','/Gorev/GorevGridPartial','GorevGrid','0')"> Sil </a>
                </td>
            </tr>
        }
    </tbody>
</table>