@model List<IR>
<table class="table table-striped table-bordered table-hover">
    <thead>
        <tr>
            <th> HesapKodu</th>
            <th> EvrakNo</th>
            <th> Depo</th>
            <th> Teslim Yeri</th>
            <th> Teslim Tarihi</th>
            <th> Kaydeden </th>
            <th> KayıtTarihi </th>
        </tr>
    </thead>
    <tbody id="IcmalFrom">
        @*@{Html.RenderAction("NewMalzemeForm", new { id = ViewBag.IrsaliyeId });}*@
        @foreach (var item in Model)
            {
            <tr>
                <td>@item.HesapKodu</td>
                <td>@item.EvrakNo</td>
                <td>@item.TK_DEP.DepoAdi</td>
                <td>@item.TeslimCHK</td>
                <td>@item.Tarih</td>
                <td>@item.Kaydeden</td>
                <td>@item.KayitTarih</td>
            </tr>
        }
    </tbody>
</table>
<script type="text/javascript">
    $("#CreateEdit").hide();
</script>
