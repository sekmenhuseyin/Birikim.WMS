@model List<CTargetRaporTemsilci>
<div class="modal-content">
    <div class="close-modal" data-dismiss="modal"><div class="lr"><div class="rl"></div></div></div>
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="modal-body">
                    <table class="table table-striped table-bordered table-condensed" id="sample_1">
                        <thead>
                            <tr>
                                <th>Temsilci</th>
                                <th>Bölge</th>
                                <th>Hedef</th>
                                <th>Hedef Oran</th>
                                <th>Toplam Iade</th>
                                <th>Net Satış Tutarı</th>
                                <th>Net Ciro</th>
                                <th>Cira Oran</th>
                                <th>Cari Borç</th>
                                <th>PRT</th>
                                <th>Bekleyen Sipariş</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                            <tr>
                                <td nowrap>@item.Temsilci</td>
                                <td nowrap>@item.GrupKod</td>
                                <td nowrap>@Math.Round(item.Hedef, 2)</td>
                                <td nowrap>@Math.Round(item.HedefOran, 2)</td>
                                <td nowrap>@Math.Round(item.ToplamIade, 2)</td>
                                <td nowrap>@Math.Round(item.NetSatisTutari, 2)</td>
                                <td nowrap>@Math.Round(item.NetCiro, 2)</td>
                                <td nowrap>@Math.Round(item.CiroOran, 2)</td>
                                <td nowrap>@Math.Round(item.CariBorc, 2)</td>
                                <td nowrap>@Math.Round(item.PRT, 2)</td>
                                <td nowrap>@Math.Round(item.BekleyenSiparis, 2)</td>
                            </tr>
                            }
                        </tbody>
                        <tfoot><tr><th colspan="11"></th></tr></tfoot>
                        <tfoot class="tf-search">
                            <tr>
                                <th></th>
                                <th></th>
                                <th></th>
                                <th></th>
                                <th></th>
                                <th></th>
                                <th></th>
                                <th></th>
                                <th></th>
                                <th></th>
                                <th></th>
                            </tr>
                        </tfoot>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>