@model IEnumerable<frmOnayTeklifList>
@{  }

    
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="modal-body">
                    @*Project Details Go Here*@

                <table class="table table-striped table-bordered" id="sample_2">
                    <thead><tr><th>Teklif No</th><th>Hesap Kodu</th><th>Çeşit</th><th>Miktar</th><th>Kaydeden</th><th>KayıtTarihi</th><th>TeklifTarihi</th></tr></thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
              
                            <tr class="clickable" onclick="satir_click('@item.TeklifNo')">
                                <td>@item.TeklifNo</td>
                                <td>@item.HesapKodu</td>
                                <td>@item.Cesit</td>
                                <td>@item.Miktar</td>
                                <td>@item.Kaydeden</td>
                                <td>@item.KayitTarihi</td>
                                <td>@item.TeklifTarihi</td>

                            </tr>
                        }
                    </tbody>
                </table>

            </div>
        </div>
    </div>
</div>

<script>
    $(document).ready(function () {
        var table = $('#sample_2').DataTable({
            "language": { "url": "http://cdn.datatables.net/plug-ins/1.10.13/i18n/Turkish.json" }
        });
    });
    function getresult(e) {
        if (e.Status == true)
            $('#modalEditPage').modal('toggle');
        else
            Modaldialog(e.Message, 'Hata', 'Tamam', 'btn-danger');
    };
</script>