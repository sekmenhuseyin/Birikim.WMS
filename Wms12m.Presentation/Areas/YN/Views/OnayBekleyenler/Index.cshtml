@{
    ViewBag.Title = "Onay Bekleyenler";
    Layout = "~/Views/Shared/_Layout.cshtml";
}<div class="page-bar"><ul class="page-breadcrumb"><li><a href="/">Anasayfa</a><i class="fa fa-circle"></i></li><li><span>Onay Bekleyenler</span></li></ul></div>
<h1 class="page-title">Onay Bekleyenler</h1>
<div class="row">
    <div class="col-md-12">
        <div class="portlet box green">
            <div class="portlet-title"><div class="caption"><i class="fa fa-gift"></i>Onay Bekleyenler</div></div>
            <div class="portlet-body form p-30 p-t-15">
                <table class="table table-striped table-bordered" id="sample_1">
                    <thead>
                        <tr>
                            <th>Evrak No</th>
                            <th>Veren</th>
                            <th>Ünvan</th>
                            <th>Borçlu</th>
                            <th>Borçlu Ünvan</th>
                            <th>Vade Tarihi</th>
                            <th>Tutar</th>
                            <th>Bağlantı Tutar</th>
                            <th>Bağlantının Ortalama Vadesi</th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>
@section Scripts{<script>
    var table = $('#sample_1').DataTable({
        "language": { "url": "http://cdn.datatables.net/plug-ins/1.10.13/i18n/Turkish.json" },
        "ordering": false,
        "ajax": {
            "url": '/YN/OnayBekleyenler/List',
            "dataSrc": function (json) {
                return json;
            },
        },
        "fixedHeader": true,
        "columns": [
            { "data": "ID" },
            { "data": "EvrakNo" },
            { "data": "Veren" },
            { "data": "Unvan" },
            { "data": "Borclu" },
            { "data": "BorcluUnvan" },
            { "data": "VadeTarihi" }
        ]
    });
</script>}