@model IEnumerable<ProjeForm>
<div class="portlet box green">
    <div class="portlet-title">
        <div class="caption">
            <i class="fa fa-gift"></i>Form Listesi
        </div>
    </div>
    <div class="portlet-body form p-40">
        <table class="table table-striped table-bordered table-hover" id="tbl_@ViewBag.id">
            <thead><tr><th>Firma</th><th>Proje</th><th>Form</th><th></th></tr></thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.Musteri.Firma</td>
                        <td>@item.ProjeForm2.Proje</td>
                        <td>@item.Form</td>
                        <td>
                            <a class="btn btn-default" href="javascript:;" onclick="CreateEditAltF('@item.ID')"> Düzenle </a>
                            <a class="btn btn-danger" href="javascript:;" onclick="Delete('@item.ID','/ToDo/ProjectForm/FormList/@ViewBag.id','listealt','')"> Sil </a>
                        
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
<script>$(document).ready(function () { $('#tbl_@ViewBag.id').DataTable({ "pagingType": "full_numbers", "language": { "url": "http://cdn.datatables.net/plug-ins/1.10.13/i18n/Turkish.json" } }); });</script>