@model IEnumerable<GorevlerCalisma>
<div class="portlet-body form">
    <table class="table table-striped table-bordered table-hover" id="sample_2">
        <thead><tr><th>Tarih</th><th>Süre</th><th>Çalışma</th><th>Kaydeden</th><th></th></tr></thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.KayitTarih.ToString("dd.MM.yyyy")</td>
                    <td>@item.Sure</td>
                    <td>@item.Calisma</td>
                    <td>@item.Kaydeden</td>
                    <td><a class="btn btn-default btn-circle" href="#modalEditPage" data-toggle="modal" onclick="editInModal('/ToDo/DutyWork/Edit/@item.ID')">Düzenle</a></td>
                </tr>
            }
        </tbody>
    </table>
</div>