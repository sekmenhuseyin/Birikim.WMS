@model IEnumerable<GetErrorLog_Result>
@{
    ViewBag.Title = "Kayıtlı Hatalar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}<div class="page-bar"><ul class="page-breadcrumb"><li><a href="/">Anasayfa</a><i class="fa fa-circle"></i></li><li><a href="/System/Logs">Kayıtlar</a><i class="fa fa-circle"></i></li><li><span>Kayıtlı Hatalar</span></li></ul></div>
<h1 class="page-title">Kayıtlı Hatalar</h1>
<div class="row">
    <div class="col-md-12">
        <div class="portlet box green">
            <div class="portlet-title"><div class="caption"><i class="fa fa-gift"></i>Kayıtlı Hatalar</div></div>
            <div class="portlet-body form p-30 p-t-15">
                <table class="table table-striped table-bordered table-hover table-responsive" id="sample_1">
                    <thead>
                        <tr>
                            <th>
                                @Html.DisplayNameFor(model => model.Description)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Message)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Source)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.DateTime)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Machine)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.IpAddress)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.UserName)
                            </th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Description)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Message)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Source)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.DateTime)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Machine)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.IpAddress)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.UserName)
                                </td>
                                <td>
                                    @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
