@model IEnumerable<GetELMAH_Error_Result>
@{
    ViewBag.Title = "Kayıtsız Hatalar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}<div class="page-bar"><ul class="page-breadcrumb"><li><a href="/">Anasayfa</a><i class="fa fa-circle"></i></li><li><a href="/System/Logs">Kayıtlar</a><i class="fa fa-circle"></i></li><li><span>Kayıtsız Hatalar</span></li></ul></div>
<h1 class="page-title">Kayıtsız Hatalar</h1>
<div class="row">
    <div class="col-md-12">
        <div class="portlet box green">
            <div class="portlet-title"><div class="caption"><i class="fa fa-gift"></i>Kayıtsız Hatalar</div></div>
            <div class="portlet-body form p-30 p-t-15">
                <table class="table table-striped table-bordered table-hover table-responsive" id="sample_1">
                    <thead>
                        <tr>
                            <th>
                                @Html.DisplayNameFor(model => model.ErrorId)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Application)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Host)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Type)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Source)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Message)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.User)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.TimeUtc)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.AllXml)
                            </th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => item.ErrorId)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Application)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Host)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Type)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Source)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Message)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.User)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.TimeUtc)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.AllXml)
                                </td>
                                <td>
                                    @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
