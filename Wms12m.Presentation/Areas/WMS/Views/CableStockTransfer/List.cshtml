@model IEnumerable<MySQLDataViewModel>
<div class="portlet box green">
    <div class="portlet-title">
        <div class="caption">
            <i class="fa fa-gift"></i>MySQL Stoklar
        </div>
    </div>
    <div class="portlet-body form p-40">
        <table class="table table-striped table-bordered" id="MySqlTable">
            <thead><tr><th></th><th nowrap>ID</th><th nowrap>SID</th><th nowrap>Marka</th><th nowrap>Cins</th><th nowrap>Kesit</th><th nowrap>Makara</th><th nowrap>Makara No</th><th nowrap>Mal Kodu</th><th nowrap>Raf</th><th nowrap>Bolum</th><th nowrap>Kat</th><th nowrap>Miktar</th><th nowrap>DepoID</th><th nowrap style="display:none">KatID</th></tr></thead>
            <tfoot><tr><th></th><th></th><th></th><th></th><th></th><th></th><th></th><th></th><th></th><th></th><th></th><th></th><th></th></tr></tfoot>
            <tfoot class="tf-search"><tr><th></th><th></th><th></th><th></th><th></th><th></th><th></th><th></th><th></th><th></th><th></th></tr></tfoot>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td></td>
                        <td nowrap>@item.MySQLID</td>
                        <td nowrap>@item.SID</td>
                        <td nowrap>@item.Marka</td>
                        <td nowrap>@item.Cins</td>
                        <td nowrap>@item.Kesit</td>
                        <td nowrap>@item.Makara</td>
                        <td nowrap>@item.MakaraNo</td>
                        <td onclick="malClick(this,@item.MySQLID)" nowrap></td>
                        <td data-itemid="" onclick="rafClick(this,@item.MySQLID)" nowrap></td>
                        <td data-itemid="" onclick="bolumClick(this,@item.MySQLID)" nowrap></td>
                        <td data-itemid="" onclick="katClick(this,@item.MySQLID)" nowrap></td>
                        <td nowrap class="text-right">@String.Format("{0:n}", item.Miktar)</td>
                        <td nowrap>@item.DepoID</td>
                        <td nowrap style="display:none"></td>
                    </tr>
                }
            </tbody>
        </table>
        <div class="form-group">
            <div class="col-sm-4 col-sm-offset-3">
                <button id="btnKaydet" onclick="btnKaydet()" class="btn btn-default btn-block btn-circle" type="button">Kaydet <i class="fa fa-save"></i></button>
            </div>
        </div>
    </div>
</div>