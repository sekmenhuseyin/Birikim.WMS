@model List<frmRezerveStok>
<div class="modal-content">
    <div class="close-modal" data-dismiss="modal"><div class="lr"><div class="rl"></div></div></div>
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="modal-body">
                    <h4>Rezerv Ayrıntı</h4>
                    <table class="table table-bordered table-condensed m-t-20">
                        <thead><tr><th>Görev No</th><th>Evrak No</th><th>HesapKodu</th><th>Unvan</th><th>Mal Kodu</th><th>Mal Adı</th><th>Rezerv Miktar</th><th>Birim</th></tr></thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>@item.GorevNo</td>
                                    <td>@item.KynkSiparisNo</td>
                                    <td>@item.HesapKodu</td>
                                    <td>@item.Unvan</td>
                                    <td>@item.MalKodu</td>
                                    <td>@item.MalAdi</td>
                                    <td>@item.Miktar</td>
                                    <td>@item.Birim</td>
                                </tr>

                            }
                        </tbody>
                    </table>
                </div>
                <div class="col-md-12">
                    <button type="button" class="btn btn-danger pull-right" data-dismiss="modal"><i class="fa fa-times"></i> Kapat</button>
                </div>
            </div>
        </div>
    </div>
</div>
