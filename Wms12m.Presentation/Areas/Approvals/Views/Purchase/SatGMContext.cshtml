
<div class="modal-content">
    <div class="close-modal" data-dismiss="modal"><div class="lr"><div class="rl"></div></div></div>
    <div class="container">
        <div class="row sonalim-text" style="width:100%;text-align:left;color:#5bc8d2;font-size:14px;margin-bottom:20px;padding:15px;"></div>
        <div id="tekliflistesi"></div>
        <div id="onaylitedarikcilistesi"></div>
        <div id="sonalimlistesi"></div>
    </div>

</div>
<script type="text/javascript">
    var type = '@ViewBag.ContextType';
    $("#tekliflistesi").html("");
    $("#onaylitedarikcilistesi").html("");
    $("#sonalimlistesi").html("");
    if (type == "teklif") {
        PartialView("/Approvals/Purchase/TeklifList/" + MLK, 'tekliflistesi');
    }
    else if (type == "tedarikci") {
        PartialView("/Approvals/Purchase/OnayliTedarikciList/" + MLK, 'onaylitedarikcilistesi');
    }
    else if (type == "sonalim") {
        $(".sonalim-text").text(MLK + " -- " + MLA)
        PartialView("/Approvals/Purchase/SonAlimListesi/" + MLK, 'sonalimlistesi');
    }
</script>
